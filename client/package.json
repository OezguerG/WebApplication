    "react-router-dom": "6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "9.16.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "22.10.1",
    "@types/react": "18.3.12",
    "@types/react-bootstrap": "0.32.37",
