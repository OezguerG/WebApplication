{
  "name": "client",
  "private": true,
  "version": "1.0.0",
  "description": "Web Engineering 2, Blatt 11",
  "type": "module",
  "scripts": {
    "abgabe": "ziplocalgit zip -f abgabe_Blatt_11.zip",
    "build": "tsc -b && vite build",
    "clean": "rimraf dist coverage junit.xml abgabe*.zip all.log",
